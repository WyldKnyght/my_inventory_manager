{
    "tables": {
      "Categories": {
        "columns": {
          "category_id": "INTEGER PRIMARY KEY AUTOINCREMENT",
          "category_name": "TEXT"
        }
      },
      "Brands": {
        "columns": {
          "brand_id": "INTEGER PRIMARY KEY AUTOINCREMENT",
          "brand_name": "TEXT"
        }
      },
      "Companies": {
        "columns": {
          "company_id": "INTEGER PRIMARY KEY AUTOINCREMENT",
          "company_name": "TEXT"
        }
      },
      "Vendor": {
        "columns": {
          "vendor_id": "INTEGER PRIMARY KEY AUTOINCREMENT",
          "company_name": "TEXT",
          "primary_contact": "TEXT",
          "email": "TEXT",
          "phone": "TEXT",
          "website": "TEXT"
        }
      },
      "Customer": {
        "columns": {
          "customer_id": "INTEGER PRIMARY KEY AUTOINCREMENT",
          "customer_first_name": "TEXT",
          "customer_last_name": "TEXT",
          "customer_email": "TEXT",
          "customer_phone": "TEXT",
          "customer_address": "TEXT"
        }
      },
      "Accounts": {
        "columns": {
          "account_id": "INTEGER PRIMARY KEY AUTOINCREMENT",
          "account_name": "TEXT",
          "account_type": "TEXT",
          "account_description": "TEXT"
        }
      },
      "Products": {
        "columns": {
          "product_id": "INTEGER PRIMARY KEY AUTOINCREMENT",
          "product_number": "TEXT UNIQUE NOT NULL",
          "product_name": "TEXT",
          "product_description": "TEXT",
          "upc": "TEXT UNIQUE",
          "company_name": "TEXT",
          "brand_name": "TEXT",
          "category_name": "TEXT",
          "theme": "TEXT",
          "product_character": "TEXT",
          "product_height": "REAL",
          "product_width": "REAL",
          "product_length": "REAL",
          "product_size": "REAL",
          "product_weight_unit": "TEXT CHECK(product_weight_unit IN ('g', 'kg', 'oz','lb'))",
          "product_color": "TEXT",
          "cost_price": "REAL",
          "selling_price": "REAL",
          "quantity": "INTEGER",
          "unit_type": "TEXT CHECK(unit_type IN ('box', 'cm', 'in', 'kg', 'lb', 'pcs'))",
          "discontinued": "BOOLEAN"
        },
        "foreign_keys": {
          "category_name": "REFERENCES Categories(category_name)",
          "company_name": "REFERENCES Companies(company_name)",
          "brand_name": "REFERENCES Brands(brand_name)"
        }
      },
      "Sales": {
        "columns": {
          "sales_id": "INTEGER PRIMARY KEY AUTOINCREMENT",
          "customer_id": "INTEGER",
          "sale_date": "DATE",
          "sub_total": "REAL",
          "discount": "REAL",
          "shipping_charges": "REAL",
          "adjustment": "REAL",
          "sales_total": "REAL",
          "customer_notes": "TEXT"
        },
        "foreign_keys": {
          "customer_id": "REFERENCES Customer(customer_id)"
        }
      },
      "Product_Sales": {
        "columns": {
          "product_sales_id": "INTEGER PRIMARY KEY AUTOINCREMENT",
          "sales_id": "INTEGER",
          "product_id": "INTEGER",
          "quantity": "INTEGER"
        },
        "foreign_keys": {
          "sales_id": "REFERENCES Sales(sales_id)",
          "product_id": "REFERENCES Products(product_id)"
        }
      },
      "Purchases": {
        "columns": {
          "purchase_id": "INTEGER PRIMARY KEY AUTOINCREMENT",
          "vendor_id": "INTEGER",
          "vendor_order_number": "TEXT",
          "purchase_order_number": "TEXT UNIQUE",
          "purchase_date": "DATE",
          "purchase_amount": "REAL",
          "taxes": "REAL",
          "discount": "REAL",
          "customs_fees": "REAL",
          "currency": "TEXT CHECK(currency IN ('CAD', 'USD'))",
          "purchase_status": "TEXT CHECK(purchase_status IN ('Pending', 'Shipped', 'Delivered'))",
          "payment_method": "TEXT",
          "shipping_cost": "REAL",
          "shipping_date": "DATE",
          "reference": "TEXT"
        },
        "foreign_keys": {
          "vendor_id": "REFERENCES Vendor(vendor_id)"
        }
      },
      "Product_Purchases": {
        "columns": {
          "product_purchase_id": "INTEGER PRIMARY KEY AUTOINCREMENT",
          "purchase_id": "INTEGER",
          "product_id": "INTEGER",
          "quantity": "INTEGER"
        },
        "foreign_keys": {
          "purchase_id": "REFERENCES Purchases(purchase_id)",
          "product_id": "REFERENCES Products(product_id)"
        }
      },
      "Expense": {
        "columns": {
          "expense_id": "INTEGER PRIMARY KEY AUTOINCREMENT",
          "expense_date": "DATE",
          "account_id": "TEXT",
          "amount": "REAL",
          "vendor_id": "INTEGER",
          "notes": "TEXT"
        },
        "foreign_keys": {
          "vendor_id": "REFERENCES Vendor(vendor_id)",
          "account_id": "REFERENCES Accounts(account_id)"
        }
      }
    }
  }